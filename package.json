{
	"name": "solstromm",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "dotenv-flow -p env -- npm-run-all --parallel dev:vite dev:smee",
		"dev:vite": "vite dev --open --host",
		"dev:smee": "smee --url https://smee.io/HIwFFAFTblPSTyS --path /api/hooks --port 5173",
		"build": "vite build",
		"preview": "dotenv-flow -p env -- vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma": "cross-env NODE_ENV=${NODE_ENV:-development} dotenv-flow -p env -- prisma"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.288.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.2.2",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"cross-env": "^7.0.3",
		"dotenv-flow": "^3.2.0",
		"dotenv-flow-cli": "^1.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"extract-zip": "^2.0.1",
		"isomorphic-fetch": "^3.0.0",
		"npm-run-all": "^4.1.5",
		"octokit": "^2.0.14",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.15.0",
		"smee-client": "^1.2.3",
		"socket.io": "^4.6.1",
		"socket.io-client": "^4.6.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3",
		"zip-a-folder": "^1.1.5"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.2.3",
		"@prisma/client": "^4.15.0",
		"@project-serum/anchor": "^0.26.0",
		"@solana/wallet-adapter-wallets": "^0.19.15",
		"@solana/web3.js": "^1.73.3",
		"@svelte-on-solana/wallet-adapter-anchor": "^1.0.16-alpha.0",
		"@svelte-on-solana/wallet-adapter-core": "^1.0.11-alpha.0",
		"@svelte-on-solana/wallet-adapter-ui": "^1.0.21-alpha.0",
		"aria-hidden": "^1.2.3",
		"dotenv": "^16.0.3",
		"svelte-floating-ui": "^1.2.8",
		"teil-ui": "^0.0.1"
	}
}
