{
	"name": "gh-app-test",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel dev:vite dev:smee",
		"dev:vite": "vite dev --open --host",
		"dev:smee": "smee --url https://smee.io/CUHMnAaUwsbE6WZ --path /api/hooks --port 5173",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"aws-sdk": "^2.1328.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"extract-zip": "^2.0.1",
		"isomorphic-fetch": "^3.0.0",
		"npm-run-all": "^4.1.5",
		"octokit": "^2.0.14",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"smee-client": "^1.2.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module"
}
